{
  "name": "Java Multi-Version Dev Container",
  "image": "mcr.microsoft.com/vscode/devcontainers/universal:focal",
  "customizations": {
    "vscode": {
      "extensions": [
        "redhat.java",
        "golang.go",
        "ms-vscode.gradle-language-service"
      ]
    }
  },
  "features": {
    "ghcr.io/devcontainers/features/java:1": {
      "version": "21",
      "jdkDistro": "tem",
      "installGradle": true,
      "gradleVersion": "7.5.1",
      "installMaven": true,
      "mavenVersion": "3.6.3",
      "installAnt": true,
      "antVersion": "1.10.12",
      "installGroovy": true,
      "groovyVersion": "2.5.22"
    }
  },
  "postCreateCommand": "source /etc/profile.d/sdkman.sh && sdk install java 21.0.3-open && sdk default java 21.0.3-open && chmod +x /workspaces/java-8-lambda-educative/scripts/detect-and-write-settings.sh && /workspaces/java-8-lambda-educative/scripts/detect-and-write-settings.sh"
  "postCreateCommand1": "source /etc/profile.d/sdkman.sh && sdk install java 17.0.13-open && sdk install java 21.0.3-open && sdk default java 21.0.3-open && chmod +x /workspaces/.devcontainer/scripts/configure-java-settings.sh && /workspaces/.devcontainer/scripts/configure-java-settings.sh",
  "mounts": [],
  "remoteUser": "vscode"
}
