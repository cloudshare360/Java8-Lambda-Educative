{
  "name": "Java Multi-Version Dev Container",
  "image": "mcr.microsoft.com/vscode/devcontainers/universal:focal",
  "customizations": {
    "vscode": {
      "extensions": [
        "redhat.java",
        "golang.go",
        "ms-vscode.gradle-language-service"
      ]
    }
  },
  "features": {
    "ghcr.io/devcontainers/features/java:1": {
      "version": "21",
      "jdkDistro": "tem",
      "installGradle": true,
      "gradleVersion": "7.5.1",
      "installMaven": true,
      "mavenVersion": "3.6.3",
      "installAnt": true,
      "antVersion": "1.10.12",
      "installGroovy": true,
      "groovyVersion": "2.5.22"
    }
  },
  "mounts": [],
  "remoteUser": "vscode"
}
